  "scripts": {
    "test": "set NODE_ENV=test&& mocha --require ./test/setup.js --compilers js:babel-core/register \"src/**/*@(.test.js|.test.jsx)\"",
    "test:dev": "webpack-dev-server --config webpack.test.config.js",
    "test:watch": "set NODE_ENV=test&& mocha --watch --require ./test/setup.js --compilers js:babel-core/register \"src/**/*@(.test.js|.test.jsx)\"",
    "win:build-development": "set NODE_ENV=development&& webpack -d --progress --config ./webpack.development.config.js",
    "win:build-production": "set NODE_ENV=production&& webpack -p --progress --config ./webpack.production.config.js",
    "win:dev-server": "set NODE_ENV=development&& webpack-dev-server --progress --config ./webpack.development.config.js",
    "lin:build-development": "NODE_ENV=development webpack -d --progress --config ./webpack.development.config.js",
    "lin:build-production": "NODE_ENV=production webpack -p --progress  --config ./webpack.production.config.js",
    "lin:dev-server": "NODE_ENV=development webpack-dev-server --progress --config ./webpack.development.config.js"
  },
